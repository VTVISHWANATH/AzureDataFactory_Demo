		{
			"name": "[concat(parameters('factoryName'), '/Dataflow_01')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "BooksCsv",
								"type": "DatasetReference"
							},
							"name": "BookCsv"
						},
						{
